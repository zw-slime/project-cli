{
  "name": "@zhangwei-smile/create-project",
  "version": "0.1.4",
  "description": "A CLI to bootstrap my new projects",
  "main": "build/src/index.js",
  "bin": {
    "zw/create-project": "bin/create-project.js",
    "create-project": "bin/create-project.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "cz",
    "build": "node build.js"
  },
  "keywords": [
    "cli",
    "create-project"
  ],
  "author": "zhangwei",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.1",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "download-git-repo": "^3.0.2",
    "envinfo": "^7.8.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "listr": "^0.14.3",
    "pkg-install": "^1.0.0",
    "semver": "^7.3.5",
    "ts-node": "^10.4.0"
  },
  "files": [
    "bin/",
    "build/",
    "CHANGELOG.md"
  ],
  "repository": "https://github.com/zw-slime/create-project-cli.git",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/envinfo": "^7.8.1",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/listr": "^0.14.4",
    "@types/node": "^16.11.6",
    "@types/semver": "^7.3.9",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "^0.13.12",
    "eslint": "^8.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src*": "prettier --write --ignore-unknown",
    "src/**/*.{ts,js}": "eslint --fix"
  }
}
